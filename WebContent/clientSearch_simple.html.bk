<!DOCTYPE html>
<html data-ng-app="clientSearchApp">
<head>
<meta charset="UTF-8">
<title>Client Search</title>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.5/angular.min.js"></script>
</head>
<script>
  var myapp=angular.module('clientSearchApp', []);
  myapp.controller('clientSearchCtrl', function($scope)
		  {
	      $scope.clientNames=['Avyan Gupta','vedant Gupta','Ashish Gupta'];
		  }
  );
</script>

<body data-ng-controller="clientSearchCtrl">
Name: <input type="text" data-ng-model="searchclientName">
{{searchclientName}}
<ul>
<li data-ng-repeat="clientName in clientNames | filter:searchclientName">{{clientName|uppercase}}
</ul>
</body>
</html>